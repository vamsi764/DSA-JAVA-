import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int[] arr={1,2,3,33,4,0};
        int max=Integer.MIN_VALUE;
        for(int num:arr)
        {
            if(num>max)
            {
                max=num;
            }
        }
        System.out.println(max);
    }
}

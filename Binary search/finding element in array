import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int[] arr={1,2,3,4,5};
        int target=9;
        int left=0;
        int result=-1;
        int right=arr.length-1;
        while(left<=right)
        {
            int mid=left+(right-left)/2;
            if(arr[mid]==target) 
            {
                result=mid;
                break;
            }
            else if(arr[mid]>target)
            {
                right=mid-1;
                
            }
            else
            {
                left=mid+1;
            }
        }
        System.out.println(result);
    }
}

import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int[] arr={5,4,3,2,1};
        ArrayList<Integer> result=new ArrayList<>();
        int max=Integer.MIN_VALUE;
        for(int i=arr.length-1;i>=0;i--)
        {
            if(arr[i]>max)
            {
                result.add(arr[i]);
                max=arr[i];
            }
        }
        System.out.println(result);
    }
}

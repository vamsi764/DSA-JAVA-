import java.util.*;
class Main {
    public static boolean isSorted(int[] arr)
    {
        for(int i=1;i<arr.length;i++)
        {
            if(arr[i-1]>arr[i]) return false;
        }
        return true;
    }
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int[] arr={1,2,3,3,4,5};
        System.out.println(isSorted(arr));
    }
}
